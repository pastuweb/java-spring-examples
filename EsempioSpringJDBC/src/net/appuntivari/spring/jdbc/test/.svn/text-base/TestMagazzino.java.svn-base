package test;

import java.util.ArrayList;
import java.util.Collections;
import java.util.List;

import org.apache.commons.logging.Log;
import org.apache.commons.logging.LogFactory;

import test.model.Magazzino;
import test.model.Scatola;

public class TestMagazzino {

	public static Log log = LogFactory.getLog(TestMagazzino.class);
	
	public static void main(String[] args) {
		
		Magazzino magazzino = new Magazzino();
		magazzino.setNome("test");
		List<Scatola> scatole = new ArrayList<Scatola>();
		scatole.add(new Scatola(10,20,30));
		scatole.add(new Scatola(40,50,60));
		scatole.add(new Scatola(70,80,90));
		
		magazzino.setScatole(scatole);
	
		log.debug("il magazzino contiene "
				+magazzino.getScatole().size()+
				" scatole");
		
	}
	
}
