package it.tcweb.model.dto;

import it.tcweb.base.model.dto.BaseObject;

public class EdizioneCorsuale extends BaseObject{

	private Sede sede;
	private Corso corso;
	private Integer numeroMassimoAllievi;
	
	public EdizioneCorsuale() {
		// TODO Auto-generated constructor stub
	}

	@Override
	public boolean equals(Object edizioneCorsuale) {						
		return getId().equals(((EdizioneCorsuale)edizioneCorsuale).getId()); 
	}
	
	public Sede getSede() {
		return sede;
	}

	public void setSede(Sede sede) {
		this.sede = sede;
	}

	public Corso getCorso() {
		return corso;
	}

	public void setCorso(Corso corso) {
		this.corso = corso;
	}

	public Integer getNumeroMassimoAllievi() {
		return numeroMassimoAllievi;
	}

	public void setNumeroMassimoAllievi(Integer numeroMassimoAllievi) {
		this.numeroMassimoAllievi = numeroMassimoAllievi;
	}		
	
}
